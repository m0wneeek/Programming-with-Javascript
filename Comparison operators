//comparison operators

let numOne = 10;
let numTwo = 12;
console.log(numOne > numTwo); //false
console.log(numOne < numTwo); //true
console.log(numOne >= numTwo); //false
console.log(numOne <= numTwo); //true
console.log(numOne == numTwo); //false


//logical operators

console.log(true && true); //true AND true

console.log(false || true); // false OR true

console.log(!false); //NOT false therefore true

